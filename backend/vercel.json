{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }